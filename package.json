{
  "name": "rfid-travel-log-app",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001 -H 0.0.0.0",
    "build": "next build",
    "start": "next start -p 3001",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
