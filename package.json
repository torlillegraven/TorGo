    "leaflet": "1.9.4",
    "next": "^14.2.33",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-leaflet": "4.2.1"
  },
  "devDependencies": {
    "@types/node": "20.11.30",
    "@types/react": "18.2.66",
    "concurrently": "^9.2.1",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "localtunnel": "^2.0.2",
    "prisma": "5.18.0",
    "typescript": "5.4.5"
  }
